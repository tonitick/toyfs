echo "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa" > a
echo "bbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbb" >> a
echo "cccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccc" >> a
echo "dddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddd" >> a
